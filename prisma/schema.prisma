generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Service {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model User {
  id             String  @id @default(auto()) @map("_id") @db.ObjectId
  active         Boolean
  email          String  @unique
  name           String
  password       String
  /// Could not determine type: the field only had null or empty values in the sample set.
  profileImgLink Json?
}
